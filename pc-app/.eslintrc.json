{
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": [ "error", { "argsIgnorePattern": "^_" } ]
    }
}